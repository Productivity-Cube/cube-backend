{
  "name": "cube-backend",
  "version": "1.0.0",
  "description": "Backend for a productivity cube",
  "main": "dist/index.js",
  "scripts": {
    "start": "node ./dist/index.js",
    "start-dev": "ts-node ./src/index.ts",
    "build": "npm run lint && npm run build:clean && npm run build:lib",
    "build:clean": "rimraf dist",
    "build:lib": "tsc --declaration",
    "test:integration": "nyc --nycrc-path .nyrc_integration npm run test:cmd -- \"tests/e2e/**/*_test.ts\"",
    "test:unit": "npm run test:cmd -- \"src/**/*.spec.ts\"",
    "lint": "tslint -p tsconfig-lint.json \"src/**/*.ts\"  \"tests/**/*.ts\"",
    "sequelize": "sequelize $1",
    "sequelize:migrate": "sequelize db:migrate",
    "sequelize:newmigration": "sequelize migration:create --name $1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Productivity-Cube/cube-backend.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Productivity-Cube/cube-backend/issues"
  },
  "homepage": "https://github.com/Productivity-Cube/cube-backend#readme",
  "devDependencies": {
    "chai": "^4.2.0",
    "chai-exclude": "^2.0.2",
    "dirty-chai": "^2.0.1",
    "expect": "^25.3.0",
    "mocha": "^7.1.1",
    "nock": "^12.0.3",
    "nyc": "^15.0.1",
    "proxyquire": "^2.1.3",
    "sequelize-cli": "^5.5.1",
    "sinon": "^9.0.2",
    "sinon-chai": "^3.5.0",
    "ts-node": "^8.8.2",
    "tslint": "^6.1.1",
    "typescript": "^3.8.3",
    "typescript-eslint-parser": "^22.0.0"
  },
  "dependencies": {
    "@types/bluebird": "^3.5.30",
    "@types/body-parser": "^1.19.0",
    "@types/chai": "^4.2.11",
    "@types/chai-as-promised": "^7.1.2",
    "@types/express": "^4.17.6",
    "@types/node": "^13.11.1",
    "@types/validator": "^13.0.0",
    "body-parser": "^1.19.0",
    "chai-as-promised": "^7.1.1",
    "class-transformer": "^0.2.3",
    "class-validator": "^0.11.1",
    "class-validator-jsonschema": "^1.3.1",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "fs": "0.0.1-security",
    "http": "0.0.1-security",
    "lodash": "^4.17.15",
    "moment": "^2.24.0",
    "mysql": "^2.18.1",
    "mysql2": "^2.1.0",
    "net": "^1.0.2",
    "rimraf": "^3.0.2",
    "routing-controllers": "^0.8.1-alpha.2",
    "sequelize": "^5.21.6",
    "sequelize-typescript": "^1.1.0",
    "tslint-clean-code": "^0.2.10",
    "tslint-config-standard": "^9.0.0",
    "tslint-consistent-codestyle": "^1.16.0",
    "tslint-immutable": "^6.0.1",
    "tslint-microsoft-contrib": "^6.2.0",
    "tslint-plugin-ikatyang": "^1.1.1",
    "tslint-sonarts": "^1.9.0",
    "uuid": "^7.0.3"
  },
  "standard": {
    "parser": "typescript-eslint-parser",
    "plugins": [
      "typescript"
    ],
    "globals": [
      "sandbox",
      "describe",
      "context",
      "before",
      "beforeEach",
      "after",
      "afterEach",
      "it",
      "expect"
    ]
  }
}
